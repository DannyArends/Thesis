issn = {0032-0781},
journal = {Plant \& cell physiology},
keywords = {Abscisic Acid,Abscisic Acid: physiology,Amino Acid Sequence,Arabidopsis,Arabidopsis Proteins,Arabidopsis Proteins: genetics,Arabidopsis: physiology,Base Sequence,Germination,Germination: physiology,Gibberellins,Gibberellins: physiology,Molecular Sequence Data,Mutation,Seeds,Seeds: physiology,Temperature},
