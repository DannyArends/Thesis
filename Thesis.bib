keywords = {Animals,Computational Biology,Computational Biology: methods,Database Management Systems,Databases,Genetic,Genomics,Genomics: methods,Humans,Information Storage and Retrieval,Mice,Software,User-Computer Interface},
month = nov,
number = {6},
pages = {532--44},
pmid = {19112082},
title = {{Solutions for data integration in functional genomics: a critical assessment and case study.}},
