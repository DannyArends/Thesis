journal = {BMC bioinformatics},
keywords = {Algorithms,Animals,Automated,Gene Expression Profiling,Gene Expression Profiling: methods,Gene Regulatory Networks,Gene Regulatory Networks: genetics,Oligonucleotide Array Sequence Analysis,Oligonucleotide Array Sequence Analysis: methods,Pattern Recognition,Rats,Sensitivity and Specificity,Software},
