journal = {Plant physiology},
keywords = {Arabidopsis,Arabidopsis: genetics,Arabidopsis: metabolism,Carbohydrate Metabolism,Chromosome Mapping,Crosses,Fructose,Fructose: metabolism,Genes,Genetic,Glucose,Glucose: metabolism,Nitrogen,Nitrogen: metabolism,Plant,Polymorphism,Quantitative Trait Loci,Starch,Starch: metabolism,Sucrose,Sucrose: metabolism},
month = aug,
