title = {{Trans-eQTLs reveal that independent genetic variants associated with a complex phenotype converge on intermediate genes, with a major role for the HLA.}},
url = {http://dx.plos.org/10.1371/journal.pgen.1002197 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3150446\&tool=pmcentrez\&rendertype=abstract},
volume = {7},
