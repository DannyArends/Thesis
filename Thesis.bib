keywords = {Alternative Splicing,Animals,Arabidopsis,Arabidopsis: genetics,DNA,Databases,Genes,Genome,Humans,Internet,Introns,Molecular Sequence Data,Nucleic Acid,Oryza sativa,Oryza sativa: genetics,Plant,Sequence Analysis},
