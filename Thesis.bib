keywords = {*Computational Biology,Animals,Computational Biology,Gene Expression Regulation,Humans,Neovascularization,Pathologic,Phenotype},
month = apr,
number = {14},
pages = {6544--9},
pmid = {20308572},
title = {{Systematic discovery of nonobvious human disease models through orthologous phenotypes.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20308572 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2851946\&tool=pmcentrez\&rendertype=abstract},
