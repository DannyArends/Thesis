keywords = {Gas Chromatography-Mass Spectrometry,Gas Chromatography-Mass Spectrometry: methods,Germination,Metabolomics,Oryza sativa,Oryza sativa: chemistry,Oryza sativa: physiology,Seeds,Seeds: chemistry,Seeds: physiology},
