journal = {Plant physiology},
keywords = {Abscisic Acid,Abscisic Acid: metabolism,Abscisic Acid: pharmacology,Analysis of Variance,Chromosome Mapping,Cluster Analysis,Ethylenes,Ethylenes: metabolism,Gene Expression Profiling,Gene Expression Regulation,Genetic Variation,Genotype,Germination,Germination: drug effects,Gibberellins,Gibberellins: metabolism,Gibberellins: pharmacology,Hot Temperature,Lettuce,Lettuce: genetics,Lettuce: metabolism,Light,Plant,Plant Growth Regulators,Plant Growth Regulators: metabolism,Plant Growth Regulators: pharmacology,Plant Proteins,Plant Proteins: genetics,Plant Proteins: metabolism,Plant: genetics,Pyridones,Pyridones: pharmacology,Quantitative Trait Loci,RNA,Seeds,Seeds: genetics,Seeds: metabolism,Signal Transduction},
