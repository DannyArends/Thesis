keywords = {Algorithms,Amino Acid Sequence,Amino Acid Substitution,Computer Simulation,Evolution,Genetic,Likelihood Functions,Markov Chains,Models,Molecular,Proteins,Proteins: chemistry,Sequence Alignment},
month = jan,
number = {1},
