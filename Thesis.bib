keywords = {5' Untranslated Regions,5' Untranslated Regions: chemistry,5' Untranslated Regions: genetics,Algorithms,Animals,Base Sequence,Computational Biology,Computational Biology: methods,Ferritins,Ferritins: genetics,Humans,Mice,Models,Molecular,Molecular Sequence Data,Nucleic Acid,Nucleic Acid Conformation,RNA,RNA: chemistry,RNA: genetics,Sequence Alignment,Sequence Homology,Transfer,Transfer: chemistry,Transfer: genetics},
