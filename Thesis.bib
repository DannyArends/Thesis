issn = {1061-4036},
journal = {Nature genetics},
keywords = {Arabidopsis,Arabidopsis: chemistry,Arabidopsis: genetics,Arabidopsis: metabolism,Chromosome Mapping,Flavonols,Flavonols: metabolism,Genes,Genetic Variation,Glucosinolates,Glucosinolates: metabolism,Mass Spectrometry,Plant,Quantitative Trait Loci},
month = jul,
