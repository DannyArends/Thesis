issn = {1553-7404},
journal = {PLoS genetics},
keywords = {Animals,Caenorhabditis elegans,Caenorhabditis elegans: genetics,Epistasis,Gene Expression Regulation,Genetic,Genotype,Messenger,Messenger: genetics,Polymorphism,Quantitative Trait Loci,RNA,Temperature},
month = dec,
number = {12},
