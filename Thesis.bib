keywords = {Algorithms,Amplified Fragment Length Polymorphism Analysis,Dominant,Genes,Genetic,Genetic Markers,Genetic Markers: genetics,Heritable,Lycopersicon esculentum,Lycopersicon esculentum: genetics,Models,Phenotype,Quantitative Trait},
