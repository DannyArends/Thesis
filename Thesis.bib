journal = {Plant physiology},
keywords = {Arabidopsis,Arabidopsis: enzymology,Arabidopsis: genetics,Arabidopsis: growth \& development,Arabidopsis: metabolism,Calmodulin,Calmodulin: metabolism,Carbon,Carbon Isotopes,Carbon: metabolism,Citric Acid Cycle,Desiccation,Electron Transport,Fatty Acids,Fatty Acids: metabolism,Gene Expression Regulation,Genes,Genetically Modified,Germination,Glutamate Decarboxylase,Glutamate Decarboxylase: metabolism,Glutamic Acid,Glutamic Acid: metabolism,Isotope Labeling,Mitochondria,Mitochondria: metabolism,Mutant Proteins,Mutant Proteins: metabolism,Nitrogen,Nitrogen: metabolism,Petunia,Petunia: enzymology,Plant,Plant: genetics,Plants,Protein Binding,Protein Structure,Seeds,Seeds: growth \& development,Seeds: metabolism,Tertiary,Up-Regulation,gamma-Aminobutyric Acid,gamma-Aminobutyric Acid: metabolism},
