issn = {0016-6731},
journal = {Genetics},
keywords = {Arabidopsis,Arabidopsis: genetics,Arabidopsis: physiology,Chromosome Mapping,Chromosome Mapping: methods,Chromosome Mapping: statistics \& numerical data,Genetic,Genetic: genetics,Genome,Genotype,Heritable,Plant,Plant: genetics,Population Dynamics,Quantitative Trait,Quantitative Trait Loci,Quantitative Trait Loci: genetics,Recombination},
month = feb,
