journal = {Bioinformatics (Oxford, England)},
keywords = {Algorithms,Chromosome Mapping,Chromosome Mapping: methods,Computational Biology,Computational Biology: methods,Humans,Internet,Quantitative Trait Loci,Software,User-Computer Interface},
month = apr,
