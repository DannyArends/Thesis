issn = {1367-4811},
journal = {Bioinformatics (Oxford, England)},
keywords = {Algorithms,Gene Expression Profiling,Genome-Wide Association Study,Genomics,Genomics: methods,Genotype,Humans,Phenotype,Polymorphism,Quantitative Trait Loci,Sensitivity and Specificity,Single Nucleotide,Specimen Handling},
month = aug,
