journal = {Genome biology},
keywords = {Databases,Enzymes,Enzymes: metabolism,Evolution,Genome,Genome: genetics,Metabolic Networks and Pathways,Molecular,Protein},
month = jan,
number = {6},
pages = {R63},
pmid = {19523219},
title = {{The conservation and evolutionary modularity of metabolism.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2718497\&tool=pmcentrez\&rendertype=abstract},
volume = {10},
year = {2009}
}
