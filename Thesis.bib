file = {:home/danny/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Avery, MacLeod, McCarty - 1944 - Studies on the chemical nature of the substance inducing transformation of pneumococcal types.pdf:pdf},
