journal = {BMC Bioinformatics},
keywords = {Algorithms,Base Pairing,Computational Biology,D,Databases,Genetic,Nucleic Acid Conformation,RNA,RNA: chemistry,RNA: genetics,Sequence,Sequence Alignment,Sequence Analysis,Software Validation,Time Factors,User-Comp,User-Computer Interface},
month = jan,
