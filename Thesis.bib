file = {:home/danny/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Blanco et al. - 2008 - Modulation of interleukin-1 transcriptional response by the interaction between VRK2 and the JIP1 scaffold protei.pdf:pdf},
issn = {1932-6203},
journal = {PloS one},
